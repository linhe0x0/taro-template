{
  "name": "minapp",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "default",
    "typescript": false,
    "css": "sass"
  },
  "scripts": {
    "dev": "npm run dev:weapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "build": "npm run build:weapp",
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "lint": "eslint --ext .js,.jsx ./src ",
    "lint:fix": "eslint --ext .js,.jsx --fix ./src"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": "npm run lint"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.0.14",
    "@tarojs/runtime": "3.0.14",
    "@tarojs/taro": "3.0.14",
    "@tarojs/react": "3.0.14",
    "react-dom": "^16.10.0",
    "react": "^16.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@sqrtthree/prettier-config": "~2.0.1",
    "@tarojs/cli": "~3.0.14",
    "@tarojs/mini-runner": "3.0.14",
    "@tarojs/webpack-runner": "3.0.14",
    "@types/react": "^16.0.0",
    "@types/webpack-env": "^1.13.6",
    "babel-preset-taro": "3.0.14",
    "eslint": "~6.8.0",
    "eslint-config-airbnb-base": "~14.2.0",
    "eslint-config-prettier": "~6.13.0",
    "eslint-config-taro": "3.0.14",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-prettier": "~3.1.4",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "~4.3.0",
    "lint-staged": "~10.4.1",
    "prettier": "~2.1.2",
    "stylelint": "15.10.1"
  }
}
